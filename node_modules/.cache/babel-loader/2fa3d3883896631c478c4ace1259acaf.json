n          <div class=\"card-body bg-opacity-10\">\r\n            <h3 class=\"card-title bg-opacity-10\">\r\n              {{ vehiculo.marca }} {{ vehiculo.modelo }} <br />\r\n              {{ vehiculo.a√±o }}\r\n            </h3>\r\n            <h4 class=\"mt-4 mb-2 precio\">COP {{ vehiculo.precio }}</h4>\r\n          </div>\r\n        </div>\r\n        <!-- SECCION DE FACTURACION -->\r\n          <div class=\"col-md-5 mt-5 align-self-center\">\r\n            <section class=\"bg-transparent bg-gradient rounded\">\r\n              <form\r\n                v-on:submit.prevent=\"publicar\"\r\n                class=\"card card-body bg-transparent bg-gradient rounded\"\r\n                enctype=\"multipart/form-data\"\r\n              >\r\n                <h1 class=\"text-center\">FACTURACION</h1>\r\n                <h2 class=\"text-center\">COMPRA VEHICULO</h2>\r\n                <br />\r\n                <h5 class=\"text-center\">Rellena la siguiente informacion personal <br> para completar la compra</h5>\r\n                <!-- Nombre -->\r\n                <div class=\"d-flex justify-content-center mb-3\">\r\n                  <label>Nombres:</label>\r\n                  <br>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control w-75 bg-transparent bg-gradient\"\r\n                    placeholder=\"Nombre\"\r\n                    v-model=\"cliente.nombre\"\r\n                  />\r\n                </div>\r\n\r\n                <!-- Apellido -->\r\n                <div class=\"d-flex justify-content-center mb-3\">\r\n                  <label>Apellidos:</label>\r\n                  <br>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control bg-transparent bg-gradient\"\r\n                    placeholder=\"Apellidos\"\r\n                    v-model=\"cliente.apellido\"\r\n                  />\r\n                </div>\r\n                <!-- Direccion -->\r\n                <div class=\"d-flex justify-content-center mb-3\">\r\n                  <label>Direccion:</label>\r\n                  <br>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control w-75 bg-transparent bg-gradient\"\r\n                    placeholder=\"Direccion\"\r\n                    v-model=\"cliente.direccion\"\r\n                  />\r\n                </div>\r\n                  <!-- Direccion -->\r\n                <div class=\"d-flex justify-content-center mb-3\">\r\n                  <label>Direccion:</label>\r\n                  <br>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control w-75 bg-transparent bg-gradient\"\r\n                    placeholder=\"Direccion\"\r\n                    v-model=\"cliente.direccion\"\r\n                  />\r\n                </div>\r\n                <input type=\"submit\" class=\"btn btn-primary\" value=\"COMPRAR\" />\r\n              </form>\r\n            </section>\r\n          </div>\r\n        </div>\r\n        \r\n        \r\n      </main>  \r\n    </body>\r\n  </template>\r\n  <script>\r\n    export default {\r\n      name: \"vehiculo_comprar\",\r\n      data() {\r\n        return {\r\n          vehiculo: [],\r\n          cliente:{\r\n              nombre:'',\r\n              apellido:'',\r\n              direccion:'',\r\n\r\n          },\r\n          id: this.$route.params.id,\r\n        };\r\n      },\r\n      created() {\r\n        console.log(this.id);\r\n        let url= 'http://127.0.0.1:8000/api/vehiculos/'+this.id;\r\n        fetch(url)\r\n          .then((respuesta) => respuesta.json())\r\n          .then((data) => (this.vehiculo = data))\r\n          .catch((error) => {\r\n            console.log(\"no se pudo \" + error);\r\n          });\r\n      },\r\n      factura(){\r\n        console.log('ya casi esta ')\r\n      }\r\n    };\r\n    </script>"]},"metadata":{},"sourceType":"module"}