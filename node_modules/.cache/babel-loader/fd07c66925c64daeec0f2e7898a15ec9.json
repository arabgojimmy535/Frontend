ata() {\r\n    return {\r\n      categoria: 0,\r\n      vehiculos: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    if(categoria)\r\n    fetch(\"http://127.0.0.1:8000/api/vehiculos/\")\r\n          .then((respuesta) => respuesta.json())\r\n          .then((data) => (this.vehiculos = data))\r\n          .catch((error) => {\r\n            console.log(\"no se pudo \" + error);\r\n          });\r\n    \r\n  },methods:{\r\n    categorizar(event){\r\n      this.categoria= event.target.value\r\n      console.log(this.categoria)\r\n      this.vehiculosCategorizados();\r\n    },\r\n    vehiculosCategorizados(){\r\n      if(this.categoria==1){\r\n        this.vehiculos=[];\r\n        var vehiculos_categoria= [];\r\n        fetch(\"http://127.0.0.1:8000/api/categoria_vehiculo/\")\r\n          .then((respuesta) => respuesta.json())\r\n          .then((data) => (this.vehiculos_categoria = data));\r\n        for(vehiculos in this.vehiculos_categoria){\r\n          console.log(vehiculos);\r\n        };\r\n    };\r\n    }\r\n  \r\n}};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}