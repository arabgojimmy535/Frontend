OP {{ vehiculo.precio }}</h4>\r\n              <router-link\r\n                :to=\"{ name: 'DetalleVehiculo', params: { id: vehiculo.id } }\"\r\n                class=\"col-6 mx-5 px-5 btn btn-primary\"\r\n                >Comprar</router-link\r\n              >\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </main>\r\n  </body>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"vehiculo\",\r\n  data() {\r\n    return {\r\n      categoria: 0,\r\n      vehiculos: [],\r\n      vehiculos_categoria:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    fetch(\"http://127.0.0.1:8000/api/vehiculos/\")\r\n          .then((respuesta) => respuesta.json())\r\n          .then((data) => (this.vehiculos = data))\r\n          .catch((error) => {\r\n            console.log(\"no se pudo \" + error);\r\n          });\r\n    \r\n  },methods:{\r\n    categorizar(event){\r\n      this.categoria= event.target.value\r\n      console.log(this.categoria)\r\n      this.vehiculosCategorizados();\r\n    },\r\n    vehiculosCategorizados(){\r\n      if(this.categoria==0){\r\n        fetch(\"http://127.0.0.1:8000/api/vehiculos/\")\r\n          .then((respuesta) => respuesta.json())\r\n          .then((data) => (this.vehiculos = data))\r\n          .catch((error) => {\r\n            console.log(\"no se pudo \" + error);\r\n          });\r\n      }\r\n      else if(this.categoria==1){\r\n        this.vehiculos=[];\r\n        fetch(\"http://127.0.0.1:8000/api/categoria_vehiculo/\")\r\n          .then((response) => console.log(response))\r\n          .then((data) => (this.vehiculos_categoria = data))\r\n          .catch((error) => {\r\n            console.log(\"no se pudo \" + error);\r\n          });\r\n\r\n          \r\n    };\r\n    }\r\n  \r\n}};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}